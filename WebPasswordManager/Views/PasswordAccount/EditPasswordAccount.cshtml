@model WebPasswordManager.Models.PasswordAccount
@{
    
}

<div>
    @using (Html.BeginForm("SaveAccount", "PasswordAccount"))
    {
        @Html.ValidationSummary() <br />
        <div class="form-group">
            @Html.LabelFor(x => x.AccountName)
            @Html.TextBoxFor(x => x.AccountName, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(x => x.Url)
            @Html.TextBoxFor(x => x.Url, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(x => x.UserName)
            @Html.TextBoxFor(x => x.UserName, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(x => x.Password)
            @Html.TextBoxFor(x => x.Password, new { @class = "form-control" })
        </div>
        <div class="form-group">
            @Html.LabelFor(x => x.AccountOwner)
            @Html.TextBoxFor(x => x.AccountOwner, new { @class = "form-control" })
            <div class="form-group">
                @Html.LabelFor(x => x.SecurityQuestions)
                @Html.TextAreaFor(x => x.SecurityQuestions, new { @class = "form-control" })
            </div>
        </div>

        @Html.ActionLink("Back", "Index", "PasswordAccount", null, new { @class = "btn btn-default" })
        <button type="submit" class="btn btn-default">Submit</button>
    }
</div>


